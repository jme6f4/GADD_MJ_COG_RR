
TITLE: CANONS_TRLBTS_2;

DATA:
	FILE = GD190618rr.csv;

VARIABLE:
	NAMES =
			ID_Fam   Wave     ID_MPL   
			DEMMAL   DEMAGE   DEMHSP   DEMRAC   
			SRPWCR  
			WASVCB   WASBLK   WASDGT   WSISUM   
			TRLATS   TRLBTS   
			CVLASF   CVLASC   CVLALF   CVLALC   
			CVLCOM   
			CANEVR   CANONS   CANFLF   
			CANFTY   CANF6M   CANAC6   
			CANYRS   
			ALCEVR   ALCONS   ALCFLF  
			ALCFTY   ALCF6M   ALCAC6  
			ALCYRS   
			SRPWCRDt WASVCBDt WASBLKDt 
			WASDGTDt WSISUMDt TRLATSDt 
			TRLBTSDt CVLASFDt CVLASCDt 
			CVLALFDt CVLALCDt CVLCOMDt   
		;
	USEVARIABLES =
		! outcome
	  		TRLBTS
  		! covariates
	  		DEMAGE
  			DEMMAL  		
  		! within predictor
  			CANONS
  			ALCONS
  		! between predictor
  			CANONSBW
  			ALCONSBW
  			;
  	USEOBSERVATION = 
  			(Wave EQ 2 AND TRLBTSDt EQ 1)
  		;
  	CLUSTER = 
  			ID_Fam
  		;
  	MISSING = .;
  	BETWEEN =   		
  			CANONSBW
  			ALCONSBW
  		;
  	WITHIN  		
  			CANONS
  			ALCONS
  			DEMAGE
  			DEMMAL
  		;  		

DEFINE: 
	! CREATE BW FAM PREDICTOR
		CANONSBW = cluster_mean (CANONS);	
		ALCONSBW = cluster_mean (ALCONS);	
	! CENTER WI FAM VARIABLES
		CENTER CANONS (GROUPMEAN);
		CENTER ALCONS (GROUPMEAN);
		CENTER DEMMAL (GRANDMEAN);
		CENTER DEMAGE (GRANDMEAN);		
	! CENTER BW FAM VARIABLES
		CENTER CANONSBW (GRANDMEAN);
		CENTER ALCONSBW (GRANDMEAN);
    
ANALYSIS:
        ITERATIONS = 1000000;
        TYPE = TWOLEVEL RANDOM;
        ALGORITHM = INTEGRATION;
        
OUTPUT:
	SAMPSTAT STDYX TECH1 CINT;

MODEL:
%WITHIN%
	TRLBTS on CANONS;
	TRLBTS on  DEMMAL DEMAGE ALCONS;	
	TRLBTS*;
	CANONS*;

	CANONS WITH DEMMAL*;
	CANONS WITH DEMAGE*;
	ALCONS WITH DEMMAL*;
	ALCONS WITH DEMAGE*;
	CANONS WITH ALCONS*;

%BETWEEN%
	TRLBTS on CANONSBW ALCONSBW;
	TRLBTS*;
	CANONSBW*;
	CANONSBW WITH ALCONSBW*;
