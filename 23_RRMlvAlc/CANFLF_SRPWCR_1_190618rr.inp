
TITLE: CANFLF_SRPWCR_1;

DATA:
	FILE = GD190618rr.csv;

VARIABLE:
	NAMES =
			ID_Fam   Wave     ID_MPL   
			DEMMAL   DEMAGE   DEMHSP   DEMRAC   
			SRPWCR  
			WASVCB   WASBLK   WASDGT   WSISUM   
			TRLATS   TRLBTS   
			CVLASF   CVLASC   CVLALF   CVLALC   
			CVLCOM   
			CANEVR   CANONS   CANFLF   
			CANFTY   CANF6M   CANAC6   
			CANYRS   
			ALCEVR   ALCONS   ALCFLF  
			ALCFTY   ALCF6M   ALCAC6  
			ALCYRS   
			SRPWCRDt WASVCBDt WASBLKDt 
			WASDGTDt WSISUMDt TRLATSDt 
			TRLBTSDt CVLASFDt CVLASCDt 
			CVLALFDt CVLALCDt CVLCOMDt   
		;
	USEVARIABLES =
		! outcome
	  		SRPWCR
  		! covariates
	  		DEMAGE
  			DEMMAL  		
  		! within predictor
  			CANFLF
  			ALCFLF
  		! between predictor
  			CANFLFBW
  			ALCFLFBW
  			;
  	USEOBSERVATION = 
  			(Wave EQ 1 AND SRPWCRDt EQ 1)
  		;
  	CLUSTER = 
  			ID_Fam
  		;
  	MISSING = .;
  	BETWEEN =   		
  			CANFLFBW
  			ALCFLFBW
  		;
  	WITHIN  		
  			CANFLF
  			ALCFLF
  			DEMAGE
  			DEMMAL
  		;  		

DEFINE: 
	! CREATE BW FAM PREDICTOR
		CANFLFBW = cluster_mean (CANFLF);	
		ALCFLFBW = cluster_mean (ALCFLF);	
	! CENTER WI FAM VARIABLES
		CENTER CANFLF (GROUPMEAN);
		CENTER ALCFLF (GROUPMEAN);
		CENTER DEMMAL (GRANDMEAN);
		CENTER DEMAGE (GRANDMEAN);		
	! CENTER BW FAM VARIABLES
		CENTER CANFLFBW (GRANDMEAN);
		CENTER ALCFLFBW (GRANDMEAN);
    
ANALYSIS:
        ITERATIONS = 1000000;
        TYPE = TWOLEVEL RANDOM;
        ALGORITHM = INTEGRATION;
        
OUTPUT:
	SAMPSTAT STDYX TECH1 CINT;

MODEL:
%WITHIN%
	SRPWCR on CANFLF;
	SRPWCR on  DEMMAL DEMAGE ALCFLF;	
	SRPWCR*;
	CANFLF*;

	CANFLF WITH DEMMAL*;
	CANFLF WITH DEMAGE*;
	ALCFLF WITH DEMMAL*;
	ALCFLF WITH DEMAGE*;
	CANFLF WITH ALCFLF*;

%BETWEEN%
	SRPWCR on CANFLFBW ALCFLFBW;
	SRPWCR*;
	CANFLFBW*;
	CANFLFBW WITH ALCFLFBW*;
